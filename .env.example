# Use official AliExpress Open API? set to "true" or "false"
USE_OFFICIAL_API=false

# Official AliExpress credentials (required only for USE_OFFICIAL_API=true)
APP_KEY=your_app_key_here
APP_SECRET=your_app_secret_here

# Affiliate / tracking
AFF_SHORT_KEY=your_aff_short_key_here
TRACKING_ID=your_tracking_id_here

# AfterShip
AFTERSHIP_KEY=your_aftership_key_here

# Cache duration in seconds (default 1800 = 30 minutes)
CACHE_DURATION=1800

# Server port
PORT=3000

# Base for affiliate short links
AFF_LINK_BASE=https://s.click
